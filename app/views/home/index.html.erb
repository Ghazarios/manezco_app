
<%= content_for :js_libs, "modernizr parallax" %>

<main class="landing-4">
<div class="parallax-window" data-parallax="scroll" data-speed="0.5" data-image-src="<%= asset_path('rrt/stock/landscape_12.jpg') %>">
  <img src="<%= asset_path('rrt/stock/landscape_12.jpg') %>" style="display: none;">

  <div class="container">
    <nav class="navbar navbar-landing  navbar-expand-lg">
      <a href="https://www.manezco.com" class="navbar-brand"><%= image_tag image_path('rrt/pyrrmid_64.png'), width: 32, height: 32, class: 'mr-2'%>ManezCo</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#standardNavbar" aria-controls="standardNavbar" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="standardNavbar">
        <ul class="nav navbar-nav ml-auto">
          <li class="nav-item ">
            <a class="nav-link" href="https://www.manezco.com">ABOUT</a>
          </li>
          <li class="nav-item ">
            <a class="nav-link" href="https://www.manezco.com">TEAM</a>
          </li>
          <li class="nav-item ">
            <a class="nav-link" href="https://www.manezco.com">CONTACT</a>
          </li>
        </ul>
      </div>
    </nav>
    <div class="row pb-5">
      <div class="col-12 col-md-7 text-white mt-5 pt-5 pb-5">
        <h1 class="text-shadow-light leading-normal">ManezCo Education App</h1>
        <p class="lead font-weight-medium mt-5 text-shadow-light">We are excited for you to try our new beta!</p>
        <%#= link_to('Sign up', new_user_registration_path, class: "btn btn-primary ml-auto btn-xlarge") %>
        <%#= link_to('Sign in', new_user_session_path, class: "btn btn-success ml-auto btn-xlarge") %>
        <%#= link_to('Logout', destroy_user_session_path, class: "btn btn-primary ml-auto btn-xlarge", method: :delete) %>
        <%= link_to('Click me! :)', welcome_dashboard_path, class: "btn btn-success ml-auto btn-xlarge") %>
      </div>
    </div>
  </div>
</div>
  
</main>

